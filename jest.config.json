{
  "testRegex": "((\\.|/*.)(spec|test))\\.js?$",
  "verbose": true,
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "moduleNameMapper": {
    "^.+\\.(css)$": "<rootDir>/app/utils/__mocks__/styleMock.js"
  },
  "setupFiles": [
    "raf/polyfill",
    "<rootDir>/app/test/jest.setup.js"
  ],
  "snapshotSerializers": ["enzyme-to-json/serializer"]
}